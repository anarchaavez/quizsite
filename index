<button class="prev-btn" onclick="changeSection(-1)">Previous</button>
<button class="next-btn" onclick="changeSection(1)">Next</button>

<script>
  let currentSection = 0;
  const sections = document.querySelectorAll('.section');
  sections[currentSection].classList.add('active');

  function changeSection(step) {
    sections[currentSection].classList.remove('active');
    currentSection += step;
    if (currentSection >= sections.length) currentSection = 0;
    if (currentSection < 0) currentSection = sections.length - 1;
    sections[currentSection].classList.add('active');
    window.scrollTo(0, 0);
  }
</script>

